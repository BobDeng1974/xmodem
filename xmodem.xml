<?xml version="1.0" encoding="UTF-8"?>
<manifest>

   <remote name="github"
           fetch="https://github.com" />

   <remote name="amazon"
           fetch="ssh://git-codecommit.us-east-1.amazonaws.com/v1/repos" />

   <default revision="master"
            remote="amazon"
            sync-j="8" />
  
   <project name="caseykelso/ringbuffer"                    remote="github"  path="ringbuffer" revision="master" />  
   <project name="mutex.io.xmodem"                          remote="amazon"  path="." revision="master" />
   <project name="google/googletest.git"                    remote="github"  path="gtest"  clone-depth="1"   revision="master"  />
   <project name="Kitware/CMake.git"                        remote="github"  path="cmake"  clone-depth="1"   revision="master"  />
   <project name="docopt/docopt.cpp"                        remote="github"  path="docopt" clone-depth="1" revision="master" />
   <project name="chriskohlhoff/asio"                       remote="github"  path="asio"   clone-depth="1" revision="master" />
</manifest>

